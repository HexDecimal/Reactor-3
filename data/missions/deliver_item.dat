=CREATE
TASK 1 "Find item."
TASK 2 "Deliver item."

=1
WAIT HAS_ITEM %ITEM_UID%
FINISH 1
EXEC TRACK_TARGET %TARGET%
JUMPIF 2 IS_PLAYER
JUMP 3

=2
WAIT CAN_SEE_TARGET %TARGET%
EXEC UNTRACK_TARGET %TARGET%
WAIT !HAS_ITEM %ITEM_UID%
FINISH 2
JUMP 5

=3
EXEC SEARCH_FOR_TARGET %TARGET%
JUMPIF 4 CAN_SEE_TARGET %TARGET%
LOOP

=4
EXEC DROP_ITEM %ITEM_UID%
WAIT !HAS_ITEM %ITEM_UID%
FINISH 2
JUMP 5

=5
COMPLETE
