=1
WAIT CAN_SEE_TARGET %TARGET%
SET STARTING_CHUNK GET_CHUNK_KEY
JUMP 2

=2
EXEC MOVE_TO_TARGET %TARGET%
JUMPIF 3 IS_IN_RANGE_OF_TARGET %TARGET% 5.
LOOP

=3
EXEC STOP
EXEC DROP_ITEM %ITEM_UID%
EXEC GIVE_MISSION kill_target %TARGET% {"target":%KILL_TARGET%,"location":%LOCATION%}
EXEC MOVE_TO_CHUNK_KEY %STARTING_CHUNK%
EXEC CREATE_MISSION retrieve_item {"item_uid":%QUEST_ITEM_UID%}
COMPLETE
